{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    'prettier/prettier': [
  'error',
  {
    'endOfLine': 'auto',
  }
],
    "class-methods-use-this": "off",
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ]
  }
}
